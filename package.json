{
  "name": "vue-audio-level-meter",
  "version": "1.0.2",
  "type": "module",
  "files": [
    "dist"
  ],
  "description": "A simple audio (peak) level meter for Vue.js",
  "main": "dist/vue-audio-level-meter.umd.cjs",
  "types": "./dist/vue-audio-level-meter.d.ts",
  "module": "dist/vue-audio-level-meter.js",
  "exports": {
    ".": {
      "import": "./dist/vue-audio-level-meter.js",
      "require": "./dist/vue-audio-level-meter.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "publish-gh-pages": "git subtree push --prefix dist origin gh-pages",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suterma/vue-audio-level-meter.git"
  },
  "keywords": [
    "audio",
    "level",
    "meter",
    "sound",
    "peak"
  ],
  "author": "Marcel Suter (https://marcelsuter.ch)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/suterma/vue-audio-level-meter/issues"
  },
  "homepage": "https://github.com/suterma/vue-audio-level-meter#readme",
  "dependencies": {
    "@vueuse/components": "^9.13.0",
    "@vueuse/core": "^9.13.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "@types/node": "^18.14.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "path": "^0.12.7",
    "vite-plugin-dts": "^2.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vue-tsc": "^1.2.0"
  }
}